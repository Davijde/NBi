<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="Acceptance Testing: query equalTo Csv" xmlns="http://NBi/TestSuite">
  <variables>
    <variable name="guid">
      <script language="c-sharp">Guid.NewGuid().ToString()</script>
    </variable>
  </variables>
	<test name="'Reseller Order Count' by year before 2006 (csv)" uid="0000">
		<system-under-test>
      <resultSet file="..\Csv\ResellerOrderCountByYearBefore2006.csv"/>
		</system-under-test>
		<assert>
			<equalTo>
				<column index="0" role="key" type="text"/>
				<column index="1" role="value" type="numeric"/>
        <resultSet>
          <row>
            <cell>CY 2005</cell>
            <cell>366</cell>
          </row>
          <row>
            <cell>CY 2006</cell>
            <cell>1015</cell>
          </row>
          <row>
            <cell>CY 2007</cell>
            <cell>1521</cell>
          </row>
          <row>
            <cell>CY 2008</cell>
            <cell>894</cell>
          </row>
        </resultSet>
			</equalTo>
		</assert>
	</test>
  <test name="'Reseller Order Count' by year before 2006 (csv) on a copy of the file" uid="0010">
    <setup>
      <file-copy name="ResellerOrderCountByYearBefore2006.csv" path="~..\Csv\{@guid}\" source-path="..\Csv\" />
    </setup>
    <system-under-test>
      <resultSet file="~..\Csv\{@guid}\ResellerOrderCountByYearBefore2006.csv"/>
    </system-under-test>
    <assert>
      <equalTo>
        <column index="0" role="key" type="text"/>
        <column index="1" role="value" type="numeric"/>
        <resultSet>
          <row>
            <cell>CY 2005</cell>
            <cell>366</cell>
          </row>
          <row>
            <cell>CY 2006</cell>
            <cell>1015</cell>
          </row>
          <row>
            <cell>CY 2007</cell>
            <cell>1521</cell>
          </row>
          <row>
            <cell>CY 2008</cell>
            <cell>894</cell>
          </row>
        </resultSet>
      </equalTo>
    </assert>
    <cleanup>
      <file-delete name="ResellerOrderCountByYearBefore2006.csv" path="~..\Csv\{@guid}\"/>
    </cleanup>
  </test>
</testSuite>
