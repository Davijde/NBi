<test name="A level named 'level' exists.">
	<description>Assert that the level '$level$' exists and is visible for end-users</description>
	<edition author="$username$" created="$now$"/>
	<system-under-test>
		<structure>	
			<level perspective="$perspective$" dimension="$dimension$" hierarchy="$hierarchy$" caption="$level$"/>
		</structure>
	</system-under-test>
	<assert>
		<exists/>
	</assert>
</test>